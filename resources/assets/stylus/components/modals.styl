.modal
	.modal-dialog
		max-width 500px 
		.modal-content
			border-radius 6px
			border none
			-webkit-box-shadow none 
			box-shadow none 
			.modal-header
				text-align center
				padding 20px 
			.modal-body
				background white 
				padding 20px
				text-align center
			.modal-footer
				border-top none 
				padding 20px 
				text-align center


// Support modal
.modal
	.g-recaptcha > div > div
		margin 0 auto
